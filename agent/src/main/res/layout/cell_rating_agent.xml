<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="@drawable/bg_cell"
    android:padding="@dimen/space_medium"
    android:layout_margin="@dimen/space_small"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <faith.changliu.orda3testing.base.widgets.CusRatingBar
        android:id="@+id/mCusRatingBar"
        app:isClickable="false"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <faith.changliu.orda3testing.base.widgets.DisplayTextView
        android:id="@+id/mCusTvRatingTraveler"
        app:hint="Traveler"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <faith.changliu.orda3testing.base.widgets.DisplayTextView
        android:id="@+id/mCusTvRatingComment"
        app:hint="Comment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</LinearLayout>